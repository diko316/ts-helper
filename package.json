{
  "name": "@dikolab/ts-helper",
  "version": "1.0.0",
  "description": "Typescript helpers for my upcoming projects",
  "publishConfig": {
    "access": "public"
  },
  "types": "index.d.ts",
  "typesVersions": {
    ">=4.2": {
      "*": [
        "dist/*"
      ]
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": "dist/index.js",
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.ts",
    "dist/**/*.cjs",
    "dist/**/*.cjs.map",
    "dist/**/*.mjs",
    "dist/**/*.mjs.map",
    "dist/*.js",
    "dist/*.js.map"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest --passWithNoTests --config=jest.config.js",
    "lint": "eslint -c .eslintrc lib/**",
    "copy-source": "cpy 'lib/**/*.ts' '!lib/**/*.spec.ts' 'dist/'",
    "clean": "rimraf dist",
    "prebundle": "npm run clean",
    "bundle": "rollup -c rollup.config.mjs",
    "postbundle": "run-s copy-source",
    "build": "run-s lint test bundle",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@diko-github:diko316/ts-helper.git"
  },
  "keywords": [
    "Typescript",
    "Helpers",
    "Utilities"
  ],
  "author": "Diko Tech Slave",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@rollup/plugin-buble": "^1.0.0",
    "@rollup/plugin-eslint": "^8.0.5",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-prettier": "^2.2.2",
    "rollup-plugin-rename": "^1.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "tslib": "^2.4.0"
  }
}
